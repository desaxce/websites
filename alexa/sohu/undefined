                                with (sf.query) {
                                    if (value == "" || value == defaultQuery) {
                                        value = manulQuery;
                                        style.color = "#666";
                                    }
                                }
                                break;
                            case 3:
                                sf.action = "http://mp3.sogou.com/music.so"
                                pid.value = "01040200"
                                rid.value = "02999999"
                                md.value = ""
                                po.value = ""
                                sayname.value = ""
                                with (sf.query) {
                                    if (value == "" || value == manulQuery) {
                                        value = defaultQuery;
                                        style.color = "#666";
                                    }
                                }
                                break;
                            case 4:
                                sf.action = "http://pic.sogou.com/pics"
                                pid.value = "01040501"
                                rid.value = "05999999"
                                md.value = ""
                                po.value = ""
