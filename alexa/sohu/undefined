				<script type="text/javascript">
					function changeGamemn(n){
						if (n == 0) {
							$("gameID").className = "titleB gametab_1";
							$("gamecontent1").className = "show";
							$("gamecontent2").className = "hidden";
						}else{
							$("gameID").className = "titleB gametab_2";
							$("gamecontent1").className = "hidden";
							$("gamecontent2").className = "show";
